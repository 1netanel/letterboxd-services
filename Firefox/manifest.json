{
    "manifest_version": 2,
    "name": "Letterboxd Services",
    "description": "Letterboxd extension that provides you torrents for movies.",
    "version": "0.6",
    "content_scripts": [
        {
            "matches": [
                "https://letterboxd.com/film/*"
            ],
            "js": [
                "src/content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
